class SmartList:
    def __init__(self, items):
        self.items = items

    def __len__(self):
        return len(self.items)

    def __getitem__(self, index):
        return self.items[index]

    def __setitem__(self, index, value):
        self.items[index] = value

    def __str__(self):
        return f"SmartList: {self.items}"

# إنشاء قائمة واختبار العمليات
slist = SmartList([10, 20, 30])
print(len(slist))       # 3
print(slist[1])         # 20
slist[1] = 50
print(slist)         # SmartList: [10, 50, 30])
