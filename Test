class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __str__(self):
        # للطباعة العادية
        return f"Person: {self.name}, Age: {self.age}"

    def __repr__(self):
        # للطباعة الفنية (تمثيل الكائن)
        return f"Person('{self.name}', {self.age})"

# إنشاء شخص واختبار الطباعة
person = Person("Alice", 30)
print(person)         # Person: Alice, Age: 30
print(repr(person))   # Person('Alice', 30)
